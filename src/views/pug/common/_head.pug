include ../mixins/_mixin
doctype html
html
    head
        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width, initial-scale=1.0, viewport-fit=cover')
        title TITLE
        link(media='all', rel='stylesheet', href='css/style.css')
        script(src="js/jquery-3.6.3.min.js", defer)
        script(src='js/jquery.main.js', defer)
    //- -var bodyClass= "home"; // add class on body
    body(class= bodyClass)
        noscript
            div Javascript must be enabled for the correct page display
        a.accessibility(href="#main") Skip to Content
        #wrapper
            block content
