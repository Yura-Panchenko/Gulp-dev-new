mixin sectionScroll(obj)
    section.section-scroll 
        .container.container-p-left
            .side-nav-block
                .side-nav-box
                    .sticky-wrap
                        .section-text
                            -if(obj.title)
                                h2!=obj.title
                            -if(obj.text)
                                .section-text__hold
                                    p!=obj.text
                        ul.side-nav
                            each el,i in obj.anchors
                                li: a.anchor.anchor-active(href="#id"+(i+1))!=el.title
                        -if(obj.button)
                            a.btn.btn-without-border.btn-underlined(href="#")!=obj.button
                .side-blocks
                    each el,i in obj.anchors
                        .block(id="id"+(i+1))
                            .img-box 
                                img(src="images/"+el.image, alt="image")
                            h3!=el.title
                            p!=el.description
                            -if(el.link)
                                .block-link-wrap
                                    a.btn.btn-without-border.btn-underlined(href="#")!=el.link
