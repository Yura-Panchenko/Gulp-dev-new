mixin rowImageText(obj)
    .row-image-text(class=obj.reverse ? 'row-image-text--reverse': '')
        -if (obj.image)
            .row-image-text__col
                .row-image-text__visual: img(src="images/"+obj.image, alt="image")
        .row-image-text__col
            .row-image-text__content
                h5!=obj.title
                p!=obj.text
                -if(obj.textRight || obj.textLeft)
                    .two-columns
                        -if( obj.textLeft)
                            .two-columns__col
                                h6 Available Sizes:
                                p!=obj.textLeft
                        -if(obj.textRight)
                            .two-columns__col
                                h6 Resolution:
                                p!=obj.textRight


